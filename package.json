  "version": "1.0.1",
  "author": "Ohno Kimiyoshi <ohno@designet.co.jp>",
  "contributors": [
    "Ohno Kimiyoshi <ohno@designet.co.jp>"
  ],
  "keywords": [
    "nextcloud",
    "mail",
    "app",
    "dev",
    "vuejs"
  ],
  "bugs": {
    "url": "https://github.com/"
  },
  "repository": {
    "url": "git+https://github.com/",
    "type": "git"
  },
  "homepage": "https://github.com/",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production webpack --progress --config webpack.js",
